{
  "name": "vitest-sonar-reporter",
  "version": "3.0.0",
  "description": "SonarQube reporter for Vitest",
  "author": "Ari Perkki√∂ <ari.perkkio@gmail.com>",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "packageManager": "pnpm@10.14.0",
  "homepage": "https://github.com/AriPerkkio/vitest-sonar-reporter",
  "bugs": "https://github.com/AriPerkkio/vitest-sonar-reporter",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AriPerkkio/vitest-sonar-reporter.git"
  },
  "scripts": {
    "changelog": "npx conventional-changelog-cli -i CHANGELOG.md -p conventionalcommits -s -r 0",
    "prebuild": "rm -rf ./dist",
    "build": "tsc --project tsconfig.prod.json",
    "start": "vitest --update",
    "lint": "eslint . --max-warnings 0 && publint",
    "test": "vitest",
    "validate": "pnpm build && pnpm lint && pnpm test run"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^62.0.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  },
  "peerDependencies": {
    "vitest": ">=3"
  },
  "keywords": [
    "vitest",
    "vitest-reporter",
    "sonar",
    "sonarqube"
  ],
  "prettier": {}
}
